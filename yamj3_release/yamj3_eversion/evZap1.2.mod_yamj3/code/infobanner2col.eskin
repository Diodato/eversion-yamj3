	<text>
		<name>outline</name>
		<segname>index</segname>
		<display>[:smartoutline,unknown:]</display>
		<wordwrap>true</wordwrap>
		<posx>35</posx>
		<posy>540</posy>
		<width>840</width>
		<height>180</height>
		<size>22</size>
		<color>FFFFFF</color>
		<hyper>3</hyper>
	</text>	
<!-- ################## LEFT: mediainfo ################## -->
	<text>
		<name>runtime</name>
		<segname>index</segname>
		<hyper>3</hyper>	
		<display>[:runtime,unknownlower:]</display>
		<posx>935</posx>
		<posy>536</posy>
		<width>160</width>
		<height>25</height>
		<size>20</size>
		<color>FFFFFF</color>
	</text>
	<text>
		<name>year_unknown</name>
		<condition>[:year:] = UNKNOWN</condition>
		<segname>index</segname>
		<hyper>3</hyper>	
		<display>&#8212;</display>
		<posx>1063</posx>
		<posy>536</posy>
		<width>160</width>
		<height>25</height>
		<size>20</size>
		<color>FFFFFF</color>
	</text>		
	<text>
		<name>year</name>
		<condition>[:year:] != UNKNOWN</condition>
		<segname>index</segname>
		<hyper>3</hyper>	
		<display>[:year:]</display>
		<posx>1060</posx>
		<posy>536</posy>
		<width>160</width>
		<height>25</height>
		<size>20</size>
		<color>FFFFFF</color>
	</text>			
	<text>
		<name>audio</name>
		<segname>index</segname>
		<hyper>3</hyper>	
		<display>[:language,upper,unknownlower,trun36:]</display>
		<posx>945</posx>
		<posy>577</posy>
		<width>300</width>
		<height>25</height>
		<size>18</size>
		<color>FFFFFF</color>
	</text>
	<text>
		<name>subt_lang0</name>
		<segname>index</segname>
		<hyper>3</hyper>	
		<condition>[:subtitles:] = NO</condition>
		<display>&#8212;</display>
		<posx>945</posx>
		<posy>611</posy>
		<width>300</width>
		<height>25</height>
		<size>18</size>
		<color>FFFFFF</color>
		<bold>true</bold>
	</text>
	<text>
		<name>subt_lang</name>
		<segname>index</segname>
		<hyper>3</hyper>	
		<condition>[:subtitles:] != NO</condition>
		<display>[:subtitles,upper,trun36:]</display>
		<posx>945</posx>
		<posy>611</posy>
		<width>300</width>
		<height>25</height>
		<size>18</size>
		<color>FFFFFF</color>
	</text>
	<!-- ################## Tile number indicators ################### -->
	<text>
		<name>tiles</name>
		<segname>index</segname>
		<hyper>4</hyper>
		<display>[:@curpos:] / [:@totaltiles:]</display>
		<align>right</align>
		<posx>1077</posx>
		<posy>488</posy>
		<width>125</width>
		<height>30</height>
		<size>20</size>
		<color>808080</color>
	</text>		
	<image>
		<name>up_on</name>
		<segname>index</segname>
		<condition>[:@curpos:] != 1</condition>
		<file>[:@media:]common/up_on.swf</file>
		<posx>1212</posx>
		<posy>495</posy>
		<width>15</width>
		<height>10.6</height>
		<hyper>4</hyper>
	</image>		
	<image>
		<name>down_on</name>
		<segname>index</segname>
		<condition>[:@curpos:] != [:@totaltiles:]</condition>
		<file>[:@media:]common/down_on.swf</file>
		<posx>1227</posx>
		<posy>495</posy>
		<width>15</width>
		<height>10.6</height>
		<hyper>4</hyper>
	</image>		

		<file>[:@media:]wall/zap_bg_infowall.swf</file>  
		<posx>885</posx>
		<posy>495</posy>
		<width>357</width>
		<height>195</height>
	</image>		
<!-- ################## Title, plot, rating, certification ################## -->
	<text>
		<name>title</name>
		<segname>index</segname>
		<display>[:fulltitle,upper,unknown:]</display>
		<posx>35</posx>
		<posy>470</posy>
		<width>976</width>
		<height>48.70</height>
		<size>26</size>
		<color>FFFFFF</color>
		<hyper>4</hyper>
	</text>	
	<image>
		<name>rating_image1</name>
		<segname>index</segname>
		<hyper>3</hyper>
		<file>[:@media:]common/star/star[:score,round:].swf</file>
		<posx>38</posx>
		<posy>510</posy>
		<width>86.5</width>
		<height>15</height>
	</image>		
	<text>
		<name>rating_genre</name>
		<segname>index</segname>
		<hyper>3</hyper>
		<!--<display>[:score:] / 10   │   [:genres,4,unknown:]</display> -->
		<display>   │   [:genres,4,unknown:]</display>
		<posx>133</posx>
		<posy>509</posy>
		<width>700</width>
		<height>25</height>
		<size>17</size>
		<color>c0c0c0</color>
	</text>

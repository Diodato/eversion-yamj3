<?xml version="1.0" encoding="UTF-8"?>
<eskin>
    <control>
		<fullscreen>true</fullscreen>
    </control>
	<!-- ########## ADD background overlay for YAMJ3 ######## -->
	<image>
		<name>bg_overlay</name>
		<file>[:@media:]home/bg_zap_home.png</file>
		<posx>-20</posx>
		<posy>445</posy>
		<width>1320</width>
		<height>245</height>
	</image>
	<background>
		<file>[:@media:]home/background/movies17.jpg</file>
	</background>
	<include>navbar</include>
	<include>clock</include>	
	<remote>
		<button>REPEAT</button>
		<action>RESET</action>
		<title>[:%reset:]</title>
	</remote>	
	<remote>
		<button>SEARCH</button>
		<action>SCREEN</action>
		<file>help/help_[:#nmtmodel,lower:]_index</file>
	</remote>	
	<segment>
		<name>index</name>
		<startx>65</startx>
		<starty>65</starty>
		<scroll>2</scroll>
		<!-- widecursor_index.400.87.6.14.FCB928.true.swf -->
		<cursor>
			<file>[:@media:]wall/indexbannercursor_[:#cursor:].swf</file>
			<above>true</above>
			<posx>-23</posx>
			<posy>-26.5</posy>
			<width>605.5</width>
			<height>167</height>
		</cursor>		
		<tile>
			<cols>2</cols>
			<rows>3</rows>
			<width>590</width>
			<height>130</height>
			<image>
				<name>banner</name>
				<file>[:@jukebox:][:banner:]</file>
				<posx>10</posx>
				<posy>6.5</posy>
				<width>539</width>
				<height>99</height>
			</image>
			<image>
				<name>banner_dim</name>
				<highlight>false</highlight>
				<file>[:@media:]wall/dim_[:#dimtile:].swf</file>
				<posx>10</posx>
				<posy>6.5</posy>
				<width>539</width>
				<height>99</height>
			</image>
			<!-- frame_386.74.3.7.212121.swf -->
			<image>
				<name>bannerframe</name>
				<file>[:@media:]wall/infobannerframe_fade[:#fadetiles,lower:].swf</file>
				<posx>7</posx>
				<posy>4</posy>
				<width>547</width>
				<height>106</height>
			</image>
			<image>
				<name>watched</name>
				<file>[:@media:]common/icon_watched_[:watched:].swf</file>
				<posx>512</posx>
				<posy>68</posy>
				<width>32</width>
				<height>32</height>
			</image>
		</tile>
		<remote>
		   <button>PLAY</button>
		   <action>PLAYALL</action>
		</remote>			
		<remote>
			<button>EQUAL</button>
			<action>EXTRA</action>	
			<file>indexinfopopup</file>
		</remote>			
		<remote>
			<button>INFO</button>
			<action>EXTRA</action>	
			<file>indexinfopopup</file>
		</remote>		
	</segment>	
<!-- ################## background icons & footer: Certification, company, mediainfo icons ################### -->

	</image>
	<!-- ## contains mediainfo flags, studio, certification -->
	<!-- not used with yamj3
	<image>
		<name>mediainfo_footer</name>
		<segname>index</segname>
		<hyper>4</hyper>
		<swap>true</swap>
		<file>[:@jukebox:][:baseFilename:].footer_set.png</file>
		<posx>152</posx>
		<posy>625</posy>
		<width>778</width>
		<height>82</height>

	<segment>
		<name>eplist</name>
		<datasource>episodes</datasource>
		<startx>0</startx>
		<starty>555</starty> 
		<endx>208</endx>
		<endy>117</endy> 
		<scroll>1</scroll>
		
		<cursor>
			<file>[:@media:]movie/cursor.208.117.2.5.C5EAFF.true.phf</file>
			<above>true</above>
			<posx>25</posx>
			<posy>0</posy>
		</cursor>		
		
		<tile>
			<cols>5</cols>
			<rows>1</rows>
			<width>256</width>
			<height>165</height>
			<image>
				<name>videoimage</name>
				<file>[:@jukebox:][:videoimage:]</file>
				<posx>26</posx>
				<posy>0</posy> 
				<width>208</width>
<!-- 			<height>117</height> -->
				<keepaspect>true</keepaspect>
				<haligned>center</haligned>
				<valigned>center</valigned>
			</image>
			<text>
				<name>epnumber</name>
				<display>[:episode:]</display>
				<align>right</align>
				<posx>0</posx>
				<posy>2</posy>
				<width>24</width>
				<height>23</height>
				<size>18</size>  
				<bold>true</bold>
				<color>tv.episode.color</color>
				<hlcolor>tv.episode.highlight.color</hlcolor>
			</text>	
			<text>
				<name>title</name>
				<display>[:title,blank:]</display>
				<align>center</align>
				<posx>0</posx>
				<posy>117</posy>
				<width>256</width>
				<height>23</height>
				<size>18</size>  
				<color>tv.episode.color</color>
				<hlcolor>tv.data.color</hlcolor>
			</text>	
			<text>
				<name>aired</name>
